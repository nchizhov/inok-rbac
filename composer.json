  }, {
    "name" : "Jesse Burns",
    "email" : "jesse.burns@alliancecms.com",
    "homepage" : "http://www.jbwebware.com",
    "role" : "Developer"
  }, {
    "name": "Chizhov Nikolay",
    "email": "nchizhov@inok.ru",
    "homepage": "http://blog.kgd.in",
    "role": "System Administrator"
  }],
  "require": {
    "php": ">=7.4.0",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.20",
    "phpunit/dbunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Inok\\RBAC\\": "src"
